    <TargetFrameworkVersion>v4.5</TargetFrameworkVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'x64.NET45' ">
    <PlatformTarget>x64</PlatformTarget>
