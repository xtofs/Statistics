  <PropertyGroup Condition=" '$(Platform)' == 'x86.NET45' ">
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <!--ending .NET45 compilation-->

  <!--starting .NET40 compilation-->
  <PropertyGroup Condition=" '$(Platform)' == 'AnyCPU.NET40' OR '$(Platform)' == 'x64.NET40' OR '$(Platform)' == 'x86.NET40' ">
    <DefineConstants>$(DefineConstants);NET30;NET35;NET40</DefineConstants>
    <TargetFrameworkVersion>v4.0</TargetFrameworkVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'x64.NET40' ">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'x86.NET40' ">
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <!--ending .NET40 compilation-->

  <!--starting .NET35 compilation-->
  <PropertyGroup Condition=" '$(Platform)' == 'AnyCPU.NET35' OR '$(Platform)' == 'x64.NET35' OR '$(Platform)' == 'x86.NET35' ">
    <DefineConstants>$(DefineConstants);NET30;NET35</DefineConstants>
    <TargetFrameworkVersion>v3.5</TargetFrameworkVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'x64.NET35' ">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'x86.NET35' ">
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <!--ending .NET35 compilation-->

  <!--starting .NET30 compilation-->
  <PropertyGroup Condition=" '$(Platform)' == 'AnyCPU.NET30' OR '$(Platform)' == 'x64.NET30' OR '$(Platform)' == 'x86.NET30' ">
    <DefineConstants>$(DefineConstants);NET30</DefineConstants>
    <TargetFrameworkVersion>v3.0</TargetFrameworkVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'x64.NET30' ">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'x86.NET30' ">
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <!--ending .NET30 compilation-->
</Project>

