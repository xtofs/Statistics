<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!--starting .NET46 compilation-->
  <PropertyGroup Condition=" '$(Platform)' == 'AnyCPU.NET46' OR '$(Platform)' == 'x64.NET46' OR '$(Platform)' == 'x86.NET46' ">
    <DefineConstants>$(DefineConstants);NET30;NET35;NET40;NET45;NET46</DefineConstants>
    <TargetFrameworkVersion>v4.6</TargetFrameworkVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'x64.NET46' ">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'x86.NET46' ">
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <!--ending .NET46 compilation-->

  <!--starting .NET45 compilation-->
